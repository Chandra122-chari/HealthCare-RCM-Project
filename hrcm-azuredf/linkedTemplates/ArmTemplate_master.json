{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hrcm-azuredf"
		},
		"hrcm_AzureDataLakeStorage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hrcmproject.dfs.core.windows.net/"
		},
		"hrcm_AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://hrcm-keyvlt.vault.azure.net/"
		},
		"hrcm_AzureDatabricks_ls_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0224-074321-1cui8ejt"
		},
		"hrcm_AzureSqlDatabase_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "hrcmsqlserver.database.windows.net"
		},
		"hrcm_AzureSqlDatabase_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"hrcm_AzureSqlDatabase_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "chandra"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "hrcm-azuredf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"hrcm_AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('hrcm_AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"hrcm_AzureDataLakeStorage_ls_properties_typeProperties_url": {
						"value": "[parameters('hrcm_AzureDataLakeStorage_ls_properties_typeProperties_url')]"
					},
					"hrcm_AzureDatabricks_ls_properties_typeProperties_existingClusterId": {
						"value": "[parameters('hrcm_AzureDatabricks_ls_properties_typeProperties_existingClusterId')]"
					},
					"hrcm_AzureSqlDatabase_ls_properties_typeProperties_server": {
						"value": "[parameters('hrcm_AzureSqlDatabase_ls_properties_typeProperties_server')]"
					},
					"hrcm_AzureSqlDatabase_ls_properties_typeProperties_database": {
						"value": "[parameters('hrcm_AzureSqlDatabase_ls_properties_typeProperties_database')]"
					},
					"hrcm_AzureSqlDatabase_ls_properties_typeProperties_userName": {
						"value": "[parameters('hrcm_AzureSqlDatabase_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}